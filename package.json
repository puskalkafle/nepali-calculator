    }
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.1.0",
    "sass": "^1.46.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "postcss": "^8.4.5",
    "push-dir": "^0.4.1"
  }
}